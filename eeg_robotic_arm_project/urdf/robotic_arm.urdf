    </visual>
    <visual>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="yellow"/>
    </visual>
    <visual>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <origin xyz="0.025 0 0" rpy="0 0 0"/>
      <material name="yellow"/>
    </visual>
  </link>

  <!-- Joint 4: Distal Interphalangeal (DIP) Flexion -->
