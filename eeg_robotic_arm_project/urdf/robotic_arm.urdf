  <!-- ======================= -->
  <!-- ====     THUMB     ==== -->
  <!-- ======================= -->
  
  <!-- Joint 1a: Thumb Opposition/Reposition (Rotation around Y axis) -->
  <joint name="thumb_opp_joint" type="revolute">
